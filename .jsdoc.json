{
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc"]
  },
  "source": {
      "include": ["README.md"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs|.test.js$)"
  },
  "plugins": [
      "plugins/markdown"
  ],
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": true,
      "useLongnameInNav": false,
      "showInheritedInNav": true
  },
  "opts": {
      "encoding": "utf8",
      "private": true,
      "recurse": true,
      "template": "./node_modules/docdash"
  }
}
